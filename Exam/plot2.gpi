set term TERM
set out 'OUT'
set xlabel 'Matrix Dimension "n"'
set ylabel 'Number of iterations'
set tics out
set key left top
set size 0.6,0.8
set yrange [:95]
plot \
 "plot2.data" using 1:2 with lines title "20 Iter. per. Upd." \
,"plot2.data" using 1:3 with lines title "40 Iter. per. Upd." \
,"plot2.data" using 1:4 with lines title "60 Iter. per. Upd." \
,"plot2.data" using 1:5 with lines title "80 Iter. per. Upd." \
